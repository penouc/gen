{
  "name": "congen",
  "displayName": "congen",
  "description": "code generator, content generator, snippet, colors generator",
  "version": "0.0.3",
  "icon": "images/G.png",
  "publisher": "penouc",
  "engines": {
    "vscode": "^1.27.0"
  },
  "repository": "https://github.com/penouc/gen",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.gen.color.dark",
    "onCommand:extension.gen.color.darkblue",
    "onCommand:extension.gen.color.greyblue",
    "onCommand:extension.gen.color.purple",
    "onCommand:extension.gen.color.greyyellow",
    "onCommand:extension.gen.color.blue",
    "onCommand:extension.gen.color.yellow",
    "onCommand:extension.gen.color.lightred",
    "onCommand:extension.gen.color.greygreen",
    "onCommand:extension.gen.color.grey",
    "onCommand:extension.gen.color.blackblue",
    "onCommand:extension.gen.color.blackyellow",
    "onCommand:extension.gen.color.green",
    "onCommand:extension.gen.geom.point",
    "onCommand:extension.gen.geom.lineString"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.gen.geom.point",
        "title": "gen: geom point"
      },
      {
        "command": "extension.gen.geom.lineString",
        "title": "gen: geom lineString"
      },
      {
        "command": "extension.gen.color.dark",
        "title": "gen: color dark"
      },
      {
        "command": "extension.gen.color.darkblue",
        "title": "gen: color darkblue"
      },
      {
        "command": "extension.gen.color.greyblue",
        "title": "gen: color greyblue"
      },
      {
        "command": "extension.gen.color.purple",
        "title": "gen: color purple"
      },
      {
        "command": "extension.gen.color.greyyellow",
        "title": "gen: color greyyellow"
      },
      {
        "command": "extension.gen.color.blue",
        "title": "gen: color blue"
      },
      {
        "command": "extension.gen.color.yellow",
        "title": "gen: color yellow"
      },
      {
        "command": "extension.gen.color.lightred",
        "title": "gen: color lightred"
      },
      {
        "command": "extension.gen.color.greygreen",
        "title": "gen: color greygreen"
      },
      {
        "command": "extension.gen.color.grey",
        "title": "gen: color grey"
      },
      {
        "command": "extension.gen.color.blackblue",
        "title": "gen: color blackblue"
      },
      {
        "command": "extension.gen.color.blackyellow",
        "title": "gen: color blackyellow"
      },
      {
        "command": "extension.gen.color.green",
        "title": "gen: color green"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
